{#
/**
 * @file entity_problem.html.twig
 * Default theme implementation to present Entity problem data.
 *
 * This template is used when viewing Entity problem pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_entity_problem()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('entity_problem') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
